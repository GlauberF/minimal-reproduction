    "scripts": {
        "ng": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng",
        "start": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng serve --open",
        "start-hmr": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng serve --configuration hmr --source-map=false --hmr-warning=false",
        "start-hmr-sourcemaps": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng serve --configuration hmr --source-map=true --hmr-warning=false",
        "build": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --dev",
        "build-stats": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
        "build-prod": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --prod",
        "build-prod-stats": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
        "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
        "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
        "serve:ssr_bak": "node dist/server",
        "build:client-and-server-bundles_bak": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --prod && node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng run vimbo:server:production --bundleDependencies all",
        "dev:ssr": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng run vimbo:serve-ssr",
        "serve:ssr": "node dist/server/main.js",
        "build:ssr": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng build --prod && node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng run vimbo:server:production",
        "prerender": "node --max_old_space_size=5072 ./node_modules/@angular/cli/bin/ng run vimbo:prerender"
    },
